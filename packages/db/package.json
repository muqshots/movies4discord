{
  "name": "@movies4discord/db",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "main": "./src/index.ts",
  "scripts": {
    "withenv": "env-cmd --silent -f ../../.env -- ",
    "withenv:dev": "env-cmd --silent -f ../../.env.dev -- ",
    "generate": "prisma generate",
    "migrate:dev": "pnpm withenv prisma migrate dev",
    "migrate:deploy": "pnpm withenv prisma migrate deploy",
    "reset": "pnpm withenv prisma migrate reset",
    "push": "pnpm withenv prisma db push",
    "pull": "pnpm withenv prisma db pull",
    "studio": "pnpm withenv prisma studio --browser none",
    "clean": "rimraf --no-glob ./tsconfig.tsbuildinfo ./.eslintcache",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "fix:all-files": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "docker": "pnpm migrate:deploy && yarn studio"
  },
  "devDependencies": {
    "@types/node": "18.16.3",
    "@typescript-eslint/eslint-plugin": "7.5.0",
    "@typescript-eslint/parser": "7.5.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "prettier": "3.2.5",
    "rimraf": "5.0.5",
    "typescript": "5.4.4"
  },
  "dependencies": {
    "@prisma/client": "^5.12.1",
    "env-cmd": "10.1.0",
    "prisma": "^5.12.1"
  },
  "type": "module"
}
