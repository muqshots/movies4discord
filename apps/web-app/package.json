{
  "name": "web-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "withenv": "env-cmd --silent -f ../../.env -- ",
    "withenv:dev": "env-cmd --silent -f ../../.env.dev -- ",
    "dev": "yarn withenv:dev next",
    "build": "NODE_OPTIONS='--tls-min-v1.2' yarn withenv next build",
    "start": "NODE_OPTIONS='--tls-min-v1.2' yarn withenv next start",
    "clean": "rimraf --no-glob ./.next ./out ./coverage ./tsconfig.tsbuildinfo ./.eslintcache",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "fix:all-files": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "postinstall": "test -n \"$SKIP_POSTINSTALL\" || yarn workspace @movies4discord/db run generate"
  },
  "dependencies": {
    "@movies4discord/db": "workspace:*",
    "@movies4discord/interfaces": "workspace:*",
    "@socialgouv/matomo-next": "1.6.1",
    "env-cmd": "10.1.0",
    "got": "12.2.0",
    "ky": "0.31.1",
    "next": "12.2.3",
    "next-auth": "4.10.2",
    "next-seo": "5.5.0",
    "plaiceholder": "2.4.0",
    "plyr-react": "4.0.0",
    "quick-lru": "6.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.4.0",
    "recoil": "0.7.4",
    "responselike": "3.0.0",
    "sharp": "0.30.7",
    "swr": "1.3.0",
    "throttle-debounce": "5.0.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.5.2",
    "@tailwindcss/line-clamp": "0.4.0",
    "@types/adm-zip": "0.5.0",
    "@types/node": "17.0.21",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/throttle-debounce": "5.0.0",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "autoprefixer": "10.4.7",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.3",
    "eslint-config-prettier": "8.5.0",
    "infer-next-props-type": "0.0.4",
    "postcss": "8.4.14",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.13",
    "rimraf": "3.0.2",
    "tailwind-scrollbar-hide": "1.1.7",
    "tailwindcss": "3.1.6",
    "typescript": "4.7.4"
  }
}
