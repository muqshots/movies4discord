{
  "name": "web-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "withenv": "env-cmd --silent -f ../../.env -- ",
    "withenv:dev": "env-cmd --silent -f ../../.env.dev -- ",
    "dev": "yarn withenv:dev next",
    "build": "NODE_OPTIONS='--tls-min-v1.2' yarn withenv next build",
    "start": "NODE_OPTIONS='--tls-min-v1.2' yarn withenv next start",
    "clean": "rimraf --no-glob ./.next ./out ./coverage ./tsconfig.tsbuildinfo ./.eslintcache",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "fix:all-files": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "postinstall": "test -n \"$SKIP_POSTINSTALL\" || yarn workspace @movies4discord/db run generate"
  },
  "dependencies": {
    "@discord/embedded-app-sdk": "1.1.2",
    "@movies4discord/db": "workspace:*",
    "@movies4discord/interfaces": "workspace:*",
    "@next-auth/prisma-adapter": "1.0.7",
    "@plaiceholder/next": "2.5.0",
    "@socialgouv/matomo-next": "1.8.1",
    "adm-zip": "0.5.12",
    "env-cmd": "10.1.0",
    "got": "12.6.1",
    "ky": "0.33.3",
    "next": "13.5.6",
    "next-auth": "4.24.7",
    "next-seo": "5.15.0",
    "plaiceholder": "2.5.0",
    "plyr-react": "5.3.0",
    "prop-types": "15.8.1",
    "quick-lru": "7.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "5.0.1",
    "recoil": "0.7.7",
    "responselike": "3.0.0",
    "sharp": "0.33.3",
    "sharp-phash": "2.1.0",
    "swr": "1.3.0",
    "throttle-debounce": "5.0.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.5.7",
    "@types/adm-zip": "0.5.5",
    "@types/node": "17.0.45",
    "@types/react": "18.2.74",
    "@types/react-dom": "18.2.23",
    "@types/throttle-debounce": "5.0.2",
    "@typescript-eslint/eslint-plugin": "7.5.0",
    "@typescript-eslint/parser": "7.5.0",
    "autoprefixer": "10.4.19",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.4",
    "eslint-config-prettier": "9.1.0",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.13",
    "rimraf": "5.0.5",
    "tailwind-scrollbar-hide": "1.1.7",
    "tailwindcss": "3.4.3",
    "typescript": "4.9.5"
  }
}
