{
  "name": "intros-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "withenv": "env-cmd --silent -f ../../.env -- ",
    "withenv:dev": "env-cmd --silent -f ../../.env.dev -- ",
    "dev": "yarn withenv:dev nodemon --watch ./src/ -e ts --exec \"tsx ./src/index.ts\"",
    "build": "tsc",
    "start": "yarn withenv node ./dist/index.js",
    "clean": "rimraf ./dist",
    "postinstall": "yarn workspace @movies4discord/db run generate"
  },
  "dependencies": {
    "@movies4discord/db": "workspace:*",
    "@movies4discord/interfaces": "workspace:*",
    "env-cmd": "10.1.0",
    "express": "4.18.2",
    "fluent-ffmpeg": "2.1.2",
    "img-hasher": "0.0.10",
    "workerpool": "6.5.0"
  },
  "devDependencies": {
    "@types/express": "4.17.17",
    "@types/node": "18.16.3",
    "@types/workerpool": "6.4.3",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "nodemon": "2.0.22",
    "rimraf": "5.0.1",
    "tsx": "3.13.0",
    "typescript": "4.9.5"
  }
}
