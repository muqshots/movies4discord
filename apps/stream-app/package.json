{
  "name": "stream-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "withenv": "env-cmd --silent -f ../../.env -- ",
    "withenv:dev": "env-cmd --silent -f ../../.env.dev -- ",
    "dev": "pnpm withenv:dev nodemon --watch ./src/ -e ts --exec \"node --loader ts-node/esm ./src/index.ts\"",
    "build": "tsc",
    "start": "pnpm withenv node ./dist/index.js",
    "clean": "rimraf ./dist"
  },
  "dependencies": {
    "cors": "2.8.5",
    "env-cmd": "10.1.0",
    "express": "4.18.2",
    "got": "12.6.0",
    "quick-lru": "6.1.2",
    "responselike": "3.0.0"
  },
  "devDependencies": {
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/node": "18.16.3",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "nodemon": "2.0.22",
    "rimraf": "5.0.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}
