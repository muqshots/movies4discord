{
  "name": "stream-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "withenv": "env-cmd --silent -f ../../.env -- ",
    "dev": "yarn withenv nodemon --watch ./src/ -e ts --exec \"node --loader ts-node/esm ./src/index.ts\"",
    "build": "tsc",
    "start": "yarn withenv node ./dist/index.js",
    "clean": "rimraf ./dist"
  },
  "dependencies": {
    "cors": "2.8.5",
    "env-cmd": "10.1.0",
    "express": "4.18.2",
    "got": "12.5.3",
    "quick-lru": "6.1.1",
    "responselike": "3.0.0"
  },
  "devDependencies": {
    "@types/cors": "2.8.13",
    "@types/express": "4.17.16",
    "@types/node": "17.0.21",
    "eslint": "8.33.0",
    "eslint-config-prettier": "8.6.0",
    "nodemon": "2.0.20",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}
