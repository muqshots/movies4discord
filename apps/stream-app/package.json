{
  "name": "stream-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "withenv": "env-cmd --silent -f ../../.env -- ",
    "dev": "yarn withenv nodemon --watch ./src/ -e ts --exec \"node --loader ts-node/esm ./src/index.ts\"",
    "build": "tsc",
    "start": "yarn withenv node ./dist/index.js"
  },
  "dependencies": {
    "cors": "2.8.5",
    "env-cmd": "10.1.0",
    "express": "4.17.3",
    "got": "12.0.1"
  },
  "devDependencies": {
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/node": "17.0.21",
    "eslint": "8.9.0",
    "eslint-config-prettier": "8.4.0",
    "nodemon": "2.0.15",
    "ts-node": "10.5.0",
    "typescript": "4.5.5"
  }
}
